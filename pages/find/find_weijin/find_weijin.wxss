.with100{
  width: 100%;
  background-color:#ffffff;
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid #dddddd;
}
.cu-form{
  display: flex;
  flex-direction: row;
  margin: 0rpx 20rpx;
}
.cu-form-title{
  margin-top: 30rpx;
  width:175rpx;
}
.select{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  color: #ffffff;
  font-size: 26rpx;
}
.bgc{
  margin-left: 9rpx;
  background-color: #a0a0a0;
  padding: 13rpx 18rpx;
  margin-top: 30rpx;
  border-radius: 13rpx;
}
.bgc:hover{
  background-color: #cb252a;
}
.cu-form-group{
  border-bottom: 1rpx solid #dddddd;
}
/* 查询按钮 */
.submit{
  width:600rpx;
  padding:20rpx 0rpx;
  text-align: center;
  font-size: 30rpx;
  color: #ffffff;
  background-color: #a0a0a0;
  border-radius: 16rpx; 
  margin: 35rpx auto;
}
.submit:hover{
  background-color: #cb252a;
}
/* 收寄原则 */
.principle{
  padding:0rpx 25rpx;
}
.principle_title{
  margin-top: 30rpx;
  margin-bottom: 20rpx;
  font-size: 30rpx;
}
.principle_cnt{
  color: #666666;
  font-size: 28rpx;
  line-height: 46rpx;
}
.text_indent{
  margin-left: 2em;
}