}
.input_cnt_ji{
  width: 480rpx;
}
.input_cnt_shou{
   width: 480rpx;
}
.name_tel{
  font-size: 30rpx;
  color: #333333;
}
.ji_shou_address{
    width: 706rpx;
    height: 300rpx;
