}
.shou_cnt{
  position: relative;
  padding: 37rpx 0;
}
/* 点击复制填写地址 */
.click_address_box_ji{
  position: absolute;
  right: 0;
  top: 52rpx;
  width: 125rpx;
  height: 77rpx;
  border-left: 1rpx solid #e1e1e1;
  text-align: center;
}
.click_address_ji_img{
  width: 36rpx;
  margin: 15rpx auto;
}
.click_address_box_shou{
  position: absolute;
  right: 0;
  top: 40rpx;
  width: 125rpx;
  height: 77rpx;
  border-left: 1rpx solid #e1e1e1;
  text-align: center;
}
.click_address_shou_img{
  width: 36rpx;
  margin: 15rpx auto;
}
.cu-list.card-menu{
  margin-left: 22rpx;
  margin-right: 22rpx;
  border-radius: 0;
}
.cu-form-group{
  margin-left: 22rpx;
  margin-right: 22rpx;
}
.text-grey, .line-grey, .lines-grey{
  color: #333333;
}
.content{
   display: flex;
  flex-direction: row;
  justify-content:space-between;
}
.input_float_right{
  display: flex;
  justify-content:flex-end;
  font-size: 28rpx;
  color: #999999;
}
.red_must{
  display: block;
  width: 72rpx;
  height: 30rpx;
  border: 1rpx solid #cb252a;
  border-radius: 14rpx;
  font-size: 24rpx;
  color: #cb252a;
  text-align: center;
  line-height: 30rpx;
  margin-top: -10rpx;
}
.cu-form-group .title{
  font-size: 26rpx;
}
.cu-list.menu>.cu-item .content{
  font-size: 26rpx;
}
/* 我已阅读并同意 */
.cu-form-group{
  justify-content: flex-start;
}
.tick{
  margin-left: 22rpx;
  margin-right: 22rpx;
}
.tick .text1{
  margin-left: 18rpx;
}

/* 底部结算部分 */
.footer{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 120rpx;
  background-color: #ffffff;
  border-top: 1rpx solid #d7d7d7;
  display: flex;
  flex-direction: row;
  padding-left: 40rpx;
  padding-right: 40rpx;
  justify-content: space-between;
  align-items:center;
}
.footer_left{
  display: flex;
  flex-direction: column;
  padding-top: 20rpx;
}
.footer_left_up{
  display: flex;
  flex-direction: row;
}
/* 预估价格 */
.footer_left_money{
  border-right: 1rpx solid #cccccc;
}
.footer_left_money text{
  color: #cb252a;
  padding-right: 16rpx;
}
/* 寄付现结 */
.jifu{
  padding-left: 12rpx;
}
.footer_left_down{
  color: #999999;
  font-size: 24rpx;
  line-height: 52rpx;
}
/* 右边的下单按钮 */
.footer_right{
  width: 196rpx;
  height: 66rpx;
  background-color: #999999;
  border-radius: 66rpx;
  color: #ffffff;
  line-height: 66rpx;
  text-align: center;
  font-size: 28rpx;
}